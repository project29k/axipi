{% extends 'BackendBundle::base.html.twig' %}

{% block body %}
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            {% embed 'BackendBundle::block_title.html.twig' %}
                {% block block_title %}
                    <span><i class="fa fa-sign-in"></i>Types</span>
                {% endblock %}
                {% block block_title_actions %}
                    <li><a href="{{ path('backend_type', {action: 'create'}) }}"><i class="fa fa-plus"></i>Create</a></li>
                {% endblock %}
            {% endembed %}
        </div>
    </div>
</div>

{% endblock %}

{% block style %}
    {{ parent() }}
{% endblock %}

{% block script %}
    {{ parent() }}
{% endblock %}
