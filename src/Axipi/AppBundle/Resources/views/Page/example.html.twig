{% extends 'AxipiAppBundle::base.html.twig' %}

{% block main %}
    <!-- App / Example -->
    <div class="row">
        <div class="col-lg-12">
            <h1>{{ page.title }}</h1>
        </div>
    </div>
    <!-- App / Example (end) -->
{% endblock %}

{% block meta %}
    {{ parent() }}
    {{ page.meta|raw }}
{% endblock %}

{% block style %}
    {{ parent() }}
    {{ page.style|raw }}
{% endblock %}

{% block script %}
    {{ parent() }}
    {{ page.script|raw }}
{% endblock %}
