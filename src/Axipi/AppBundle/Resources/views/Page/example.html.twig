{% extends 'AxipiAppBundle::base.html.twig' %}

{% block main %}
    <!-- App / Example -->
    <section class="main">
        <header>
            <h1>{{ page.title }}</h1>
        </header>
    </section>
    <!-- App / Example (end) -->
{% endblock %}

{% block meta %}
    {{ parent() }}
    {{ page.meta|raw }}
{% endblock %}

{% block style %}
    {{ parent() }}
    {{ page.style|raw }}
{% endblock %}

{% block script %}
    {{ parent() }}
    {{ page.script|raw }}
{% endblock %}
