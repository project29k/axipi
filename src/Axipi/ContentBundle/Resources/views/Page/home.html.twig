{% extends 'AxipiContentBundle::base.html.twig' %}

{% block main %}
    <!-- Content / Home -->
    <section class="main">
        <header>
            <h1>{{ page.title }}</h1>
        </header>

        <article>
            {{ convertText(page.attribute('description')|raw) }}
        </article>
    </section>
    <!-- Content / Home (end) -->
{% endblock %}

{% block meta %}
    {{ parent() }}
    {{ page.meta|raw }}
{% endblock %}

{% block style %}
    {{ parent() }}
    {{ page.style|raw }}
{% endblock %}

{% block script %}
    {{ parent() }}
    {{ page.script|raw }}
{% endblock %}
