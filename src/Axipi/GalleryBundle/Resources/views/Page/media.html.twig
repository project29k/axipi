{% extends 'AxipiGalleryBundle::base.html.twig' %}

{% block main %}
    <!-- Gallery / Media -->
    <div class="row">
        <div class="col-lg-12">
            <h1>media controller</h1>
            <h2>{{ page.title }}</h2>
            <p><a href="{{ path('axipi_core_slug', {slug: 'a/b/c/d'}) }}">a/b/c/d</a></p>
            <p><a href="{{ path('axipi_core_slug', {slug: 'axipi'}) }}">axipi</a></p>
            <p><img src="{{ asset('uploads/' ~ page.attribute('image')) }}"></p>
        </div>
    </div>
    <!-- Gallery / Media (end) -->
{% endblock %}

{% block style %}
    {{ parent() }}
{% endblock %}

{% block script %}
    {{ parent() }}
{% endblock %}
